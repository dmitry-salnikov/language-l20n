'scopeName': 'source.l20n'
'name': 'L20n'
'fileTypes': [
  'l20n'
]
'patterns': [
  {
    'name': 'comment.block'
    'begin': '/\\*'
    'end': '\\*/'
  }
  {
    'name': 'entity.name.section'
    'begin': '<(\\w+)'
    'end': '>'
    'captures':
      1: 'name': 'entity.name.tag'
    'patterns': [
      {
        'include': '#strings'
      }
    ]
  }
]
'repository':
  'strings':
    'patterns': [
      {
        'name': 'string.quoted.triple'
        'begin': '"""'
        'end': '"""'
      }
      {
        'name': 'string.quoted'
        'begin': '("|\')'
        'end': '("|\')'
      }
    ]
